<config>
    <modules>
        <Sekizgen_Carousel>
            <version>0.0.1</version>
        </Sekizgen_Carousel>
    </modules>
    <global>
        <helpers>
            <carousel>
                <class>Sekizgen_Carousel_Helper</class>
            </carousel>
        </helpers>
        <blocks>
             <carousel>
                <class>Sekizgen_Carousel_Block</class>
             </carousel>
        </blocks>
        <models>
             <carousel>
                <class>Sekizgen_Carousel_Model</class>
             </carousel>
        </models>
    </global>






    <frontend>
      <translate>
          <modules>
              <carousel>
                  <files>
                      <default>Sekizgen_Carousel.csv</default>
                  </files>
              </carousel>
          </modules>
      </translate>
    </frontend>





</config>
